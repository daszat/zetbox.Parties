<?xml version="1.0" encoding="Windows-1252"?>
<ZetboxConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dasz.at/Zetbox/">
  <ConfigName>Zetbox.Parties Server Configuration</ConfigName>
  <Server StartServer="true">
    <ConnectionStrings>
      <Database Name="Zetbox" Schema="MSSQL" Provider="System.Data.SqlClient">Data Source=.\SQLEXPRESS;Initial Catalog=Parties;Integrated Security=True;MultipleActiveResultSets=true;</Database>
    </ConnectionStrings>
    <DocumentStore>DocumentStore\Server\</DocumentStore>
    <Modules>
      <Module>Zetbox.Server.ServerModule, Zetbox.Server</Module>
      <Module>Zetbox.DalProvider.Ef.EfProvider, Zetbox.DalProvider.Ef</Module>
      <Module NotOnFallback="true">zetbox.Workflow.Server.ServerModule, zetbox.Workflow.Server</Module>
      <Module NotOnFallback="true">Zetbox.Parties.Server.ServerModule, Zetbox.Parties.Server</Module>
    </Modules>
    <ClientFilesLocations />
  </Server>
  <AssemblySearchPaths EnableShadowCopy="true">
    <string>Common\Core</string>
    <string>Server\Core</string>
    <string>Server\EF</string>
    <string>Common\App.Zetbox</string>
    <string>Server\App.Zetbox</string>
    <string>Common\App.Zetbox.Parties</string>
    <string>Server\App.Zetbox.Parties</string>
  </AssemblySearchPaths>
</ZetboxConfig>