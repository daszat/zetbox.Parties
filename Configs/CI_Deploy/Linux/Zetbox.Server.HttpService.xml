<?xml version="1.0" encoding="utf-8" ?>
<ZetboxConfig xmlns="http://dasz.at/Zetbox/">
  <ConfigName>Zetbox.Parties HttpService Configuration</ConfigName>
  <Server StartServer="true">
    <DocumentStore>..\DocumentStore\Server\</DocumentStore>
    <Modules>
      <Module>Zetbox.Server.ServerModule, Zetbox.Server</Module>
      <Module>Zetbox.DalProvider.NHibernate.NHibernateProvider, Zetbox.DalProvider.NHibernate</Module>
      <Module>Zetbox.Server.HtpasswdIdentitySource+Module, Zetbox.Server</Module>
      <Module>Zetbox.Parties.Server.ServerModule, Zetbox.Parties.Server</Module>
    </Modules>
    <ClientFilesLocations>
      <Location Name="Exe" Start="true">..\Zetbox.Client.WPF.exe</Location>
      <Location Name="Configs">..\Configs\Zetbox.Client.WPF.xml</Location>
      <Location Name="Common">..\Common</Location>
      <Location Name="Client">..\Client</Location>
    </ClientFilesLocations>
  </Server>
  <AssemblySearchPaths>
    <string>../Common/Core</string>
    <string>../Server/Core</string>
    <string>../Server/NH</string>
    <string>../Common/App.Zetbox</string>
    <string>../Server/App.Zetbox</string>
    <string>../Server/App.Zetbox.Parties</string>
    <string>../Common/App.Zetbox.Parties</string>
  </AssemblySearchPaths>
</ZetboxConfig>